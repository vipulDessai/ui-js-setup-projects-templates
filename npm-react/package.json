{
  "name": "vite-vitest-template",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:vite": "tsc -b && vite build",
    "build:tsx": "tsc",
    "build:webpack": "webpack",
    "lint": "eslint .",
    "preview": "vite preview",
    "pretty": "prettier --write ./**/* --ignore-unknown",
    "prettier:watch": "onchange \"./**/*\" -- prettier --write --ignore-unknown {{changed}}",
    "build:js": "tsc --jsx preserve -t es2020 --outDir dist --noEmit false src/**/*.{tsx,ts} && cp -r src/**/*.css dist/",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "sonar:scan": "node sonar-project.mjs",
    "valid:tsc": "tsc --project tsconfig.json",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.3",
    "@mui/material": "^6.1.3",
    "@reduxjs/toolkit": "^2.3.0",
    "axios": "^1.7.7",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.13.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-istanbul": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "globals": "^15.11.0",
    "jsdom": "^25.0.1",
    "mini-css-extract-plugin": "^2.9.1",
    "onchange": "^7.1.0",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "postcss-prefix-selector": "^2.1.0",
    "prettier": "^3.3.3",
    "sonarqube-scanner": "^4.2.6",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.13",
    "ts-loader": "^9.5.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.9",
    "vitest": "^2.1.8",
    "webpack": "^5.95.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4"
  },
  "description": "My webpack project"
}
